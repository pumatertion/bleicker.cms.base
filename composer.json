		"namelesscoder/fluid": "dev-master",
		"filp/whoops": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"authors": [
		{
			"name": "Carsten Bleicker",
			"email": "carsten@bleicker.de"
		}
